{
  "name": "Job apply",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Automatically apply to a job",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/jobs/search/*"],
      "js": ["content.js"]
    }
  ]
}
